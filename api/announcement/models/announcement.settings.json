{
  "connection": "default",
  "collectionName": "announcements",
  "info": {
    "name": "Announcement"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "AnnouncementTitle": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "richtext",
      "required": true
    },
    "PublishedDate": {
      "type": "date"
    },
    "ExpiredDate": {
      "type": "date"
    },
    "EventEndDate": {
      "type": "date"
    },
    "EventStartDate": {
      "type": "date"
    },
    "AnnouncementsImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "AnnouncementLink": {
      "type": "string",
      "required": true
    }
  }
}
