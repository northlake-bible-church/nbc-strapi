{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "Title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "PublishDate": {
      "type": "date",
      "required": true
    },
    "Article": {
      "type": "richtext"
    },
    "ExpiredDate": {
      "type": "date"
    },
    "Link": {
      "unique": true,
      "required": true,
      "type": "string"
    }
  }
}
