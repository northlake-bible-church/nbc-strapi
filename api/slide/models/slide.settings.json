{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "slides",
  "info": {
    "name": "slide"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "SlideName": {
      "type": "string"
    },
    "SlideImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false,
      "sortable": true
    },
    "AlternativeText": {
      "type": "string"
    },
    "Caption": {
      "type": "string"
    },
    "SlideUrl": {
      "type": "string"
    },
    "PublishDate": {
      "type": "date"
    },
    "ExpiredDate": {
      "type": "date"
    },
    "text_color": {
      "model": "text-color"
    },
    "ButtonLinkText": {
      "type": "string"
    }
  }
}
