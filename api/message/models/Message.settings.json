{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "message",
  "info": {
    "name": "message",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ],
    "draftAndPublish": false
  },
  "attributes": {
    "MessageTitle": {
      "type": "string",
      "required": true
    },
    "teachers": {
      "collection": "teacher",
      "via": "messages",
      "dominant": true
    },
    "biblebooks": {
      "collection": "biblebook"
    },
    "ChapterStart": {
      "type": "integer",
      "min": 1
    },
    "ChapterEnd": {
      "type": "integer",
      "min": 1
    },
    "VerseStart": {
      "type": "integer"
    },
    "VerseEnd": {
      "type": "integer"
    },
    "MessageDate": {
      "type": "date",
      "required": true
    },
    "PublishDate": {
      "type": "date"
    },
    "MessageSlides": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "pages": {
      "collection": "page",
      "via": "messages",
      "dominant": true
    },
    "seriesset": {
      "model": "seriesset",
      "via": "message"
    },
    "PodcastLink": {
      "type": "string"
    },
    "MessageAudio": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "MessageVideo": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "ExpireDate": {
      "type": "date"
    },
    "MainPage": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
