{
  "connection": "default",
  "collectionName": "message",
  "info": {
    "name": "message",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "MessageTitle": {
      "type": "string",
      "required": true
    },
    "teacher": {
      "model": "teacher",
      "via": "message"
    },
    "AudioLink": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "biblebooks": {
      "collection": "biblebook"
    },
    "ChapterStart": {
      "type": "integer",
      "min": 1
    },
    "ChapterEnd": {
      "type": "integer",
      "min": 1
    },
    "VerseStart": {
      "type": "integer"
    },
    "VerseEnd": {
      "type": "integer"
    },
    "MessageDate": {
      "type": "date",
      "required": true
    },
    "PublishDate": {
      "type": "date"
    },
    "VideoLink": {
      "type": "string"
    },
    "MessageSlides": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "pages": {
      "collection": "page",
      "via": "messages",
      "dominant": true
    },
    "seriesset": {
      "model": "seriesset",
      "via": "message"
    },
    "PodcastLink": {
      "type": "string"
    }
  }
}
